import requests
import re

r = requests.get('http://www.cyberoro.com/column/column_view.oro?group=38&div=85&column_no=15591&pageNo=1&m_div=A2')
r.encoding = 'euc-kr'

a = r.text
a = a[49400:-28320]
a = a.replace('<br />','\n')

a = a.replace('<strong>','댓글 닉네임 :')
a = a[:a.find('</span>')] + a[a.find('꼬릿글 쓰기</b></td>'):]

a = re.sub('<.+?>','',a)

a = a.replace('&nbsp;',' ')

print(a)
